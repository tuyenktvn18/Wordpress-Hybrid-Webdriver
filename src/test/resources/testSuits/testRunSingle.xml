<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Regression" >

    <parameter name="envName" value="local"></parameter>

    <listeners>
        <listener class-name="commons.reportConfig.MethodListener" />
        <listener class-name="commons.reportConfig.AllureTestListener" />
    </listeners>
    <test name="Categories" >
        <parameter name="browser" value="chrome"/>
        <parameter name="urlUser" value="http://localhost:8080/seleniumPractice/"/>
        <parameter name="urlAdmin" value="http://localhost:8080/seleniumPractice/wp-login.php"/>
        <classes>
            <class name="com.wordpress.testCases.admin.Media_01_CRDS" />
        </classes>
    </test>
</suite>
